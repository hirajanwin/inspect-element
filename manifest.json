{
  "manifest_version": 2,
  "version": "0.0.1",
  "name": "Inspect Element",

  "description": "Inspect element.",

  "permissions": ["storage"],

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "browser_action": {
    "default_popup": "src/browser-action/dist/index.html"
  },

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["dist/contentScript.js"]
    }
  ]
}
